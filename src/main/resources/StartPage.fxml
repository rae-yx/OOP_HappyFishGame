<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="startPageAp" prefHeight="600.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="StartPageController">
   <children>
      <ImageView fitHeight="600.0" fitWidth="400.0" pickOnBounds="true" preserveRatio="true">
      <image>
         <Image url="@startpage.png" />
      </image>
      </ImageView>
      <Button fx:id="startButton" layoutX="100.0" layoutY="388.0" onAction="#startGame" prefHeight="66.0" prefWidth="200.0" style="-fx-background-color: #facdc8;" text="START GAME">
         <font>
            <Font name="Comic Sans MS" size="24.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
